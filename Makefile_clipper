OS_NAME=$(shell uname)
ifeq "$(OS_NAME)" "Linux"
  include $(SC_BUILD_HOME_DIR)/sclib/Makefile_clipper_linux
else
  include /sclib/Makefile_clipper_cygwin
endif


all:
	lib
	
clean:
	rm -f *~
	rm -f *.xref
	rm -f *.xrefs
	rm -f *.html
	rm -f *.OBJ *.obj *.o
	rm -f xref_files
	rm -f $(OBJS) *.bak *.BAK


